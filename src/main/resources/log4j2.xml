<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} (%F:%L) - %msg%n" />
        </Console>
        <!--
        <File name="MyFile" fileName="logs/testing.log" immediateFlush="false" append="false">
            <PatternLayout pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} (%F:%L) - %msg%n"/>
        </File>
        -->
    </Appenders>
    <Loggers>
        <Root level="WARN">
            <AppenderRef ref="Console" />
            <!--
            <AppenderRef ref="MyFile"/>
            -->
        </Root>
    </Loggers>
</Configuration>